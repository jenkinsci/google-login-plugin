<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form" xmlns:c="/lib/credentials" xmlns:i="jelly:fmt" xmlns:p="/lib/hudson/project">
  <f:entry title="${%Client Id}" field="clientId">
    <f:textbox autocomplete="off"/>
  </f:entry>
  <f:entry title="${%Client Secret}" field="clientSecret">
    <f:password autocomplete="off"/>
  </f:entry>
  <f:entry title="${%Google Apps Domain}" field="domain">
    <f:textbox/>
  </f:entry>
  <f:entry title="${%Use Root URL from request}" field="rootURLFromRequest">
    <f:checkbox/>
  </f:entry>
  <f:entry title="${%GSuite Admin SDK Credentials}" field="gsuiteServiceAccountCredentialsId">
    <c:select />
  </f:entry>
  <f:entry title="${%GSuite Impersonation Account}" field="gsuiteImpersonationAccount">
    <f:textbox autocomplete="off"/>
  </f:entry>
</j:jelly>
